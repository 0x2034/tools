cat httpx.txt| waybackurls > wayback.txt
katana -list httpx.txt > katana.txt
gospider -S httpx.txt | sed -E 's/\[[^]]*\]//g; s/-//g; s/\[from://g; s/\].*//' | sed -E 's/^[ \t]+//; s/[ \t]+$//' | grep -v '^$'  > gospider.txt
cat wayback.txt katana.txt gospider.txt | anew > all_urls.txt
cat all_urls.txt | grep '.\php' > php_urls.txt
arjun -i php_urls.txt > arjun_php.txt
cat ../arjun_php.txt | sed -r 's/\x1B\[[0-9;]*[mK]//g' | awk 'BEGIN{IGNORECASE=1}/\[\*\] Scanning/{if(url&&length(param_list)>0){output=url (index(url,"?")?"&":"?");first=1;for(p in param_list){output=output (first?"":"&") p "=1";first=0}print output}url=$NF;delete seen;delete param_list}/Extracted [0-9]+ parameters.*: /{match($0,/: (.*)/,m);n=split(m[1],tmp,/[, ]+/);for(i=1;i<=n;i++){p=tolower(tmp[i]);if(!(p in seen)){seen[p]=1;param_list[p]=1}}}/\[!\] parameters found:/{match($0,/parameters found:[[:space:]]*(.*)/,m);n=split(m[1],tmp,/[, ]+/);for(i=1;i<=n;i++){p=tolower(tmp[i]);if(!(p in seen)){seen[p]=1;param_list[p]=1}}}END{if(url&&length(param_list)>0){output=url (index(url,"?")?"&":"?");first=1;for(p in param_list){output=output (first?"":"&") p "=1";first=0}print output}}' > final_php_urls.txt

cat all_urls.txt | grep '.\js' > js_urls.txt
cat js_urls.txt | mantra  -> api keys
for i in $(cat js_urls.txt); do
  echo -e "\033[36m----------------------------------------------------------------------\033[0m"
  echo -e "\033[36m[+]-- Scanning $i --[+]\033[0m"
  echo -e "\033[36m----------------------------------------------------------------------\033[0m"
  echo -e "\e[1;33mLooking for URLs ...\e[0m"
  echo ""
  jsluice urls $i 2>/dev/null
  echo -e "\033[35m----------\033[0m"
  echo -e "\e[1;33mLooking for Secrets ...\e[0m"
  echo ""
  jsluice secrets $i 2>/dev/null
  echo ""
done

